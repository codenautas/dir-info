{
  "name": "dir-info",
  "description": "Abundant dir info - with git info ",
  "version": "1.0.11",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "license": "MIT",
  "repository": "codenautas/dir-info",
  "contributors": [{
    "name": "Emilio Platzer", "email": "emilioplatzer@gmail.com"
  },{
    "name": "Diego Florio", "email": "diegoefe@gmail.com"
  }],
  "dependencies": {
    "lodash": "4.17.3",
    "fs-extra": "1.0.0",
    "fs-promise": "1.0.0",
    "child-process-promise": "2.2.0",
    "npm-check-updates": "2.8.9",
    "read-yaml-promise": "1.0.2",

    "expect-called": "0.4.0"
  },
  "devDependencies": {
    "istanbul": "0.3.20",
    "mocha": "2.3.2",
    "expect.js": "0.3.1",
    "memoizee": "0.3.9",
    "moment": "2.10.6"
  },
  "main": "dir-info.js",
  "engines": {
    "node": ">= 0.10.0"
  },
  "files": [
    "dir-info.js"
  ],
  "scripts": {
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "start": "node example/example.js"
  },
  "jshintConfig": {
    "asi": false,
    "curly": true,
    "forin": true
  },
  "codenautas": {
    "run-in": "server",
    "test-appveyor": true
  },
  "jshintConfig": {
    "asi": false,
    "forin": true,
    "curly": true,
    "esversion": 6
  },
 "eslintConfig":{
    "env": {
      "node": false
    },
    "rules": {
      "strict": 0,
      "no-console": 1,
      "no-unused-vars": 1
    },
    "parserOptions": {
      "ecmaVersion": 6
    }
  },
  "qa-control": {
    "package-version": "0.2.0",
    "run-in": "server",
    "stability": "extending",
    "test-appveyor": true,
    "type": "lib",
    "coverage": 90,
    "ecmaVersion": 6
  }
}
